{
    "batch_size": 2,
    "max_step": 10,
    "log_path": "examples/hotpotqa/logs",

    "sample_kind": "random",
    "allow_duplicate_samples": false,
    "has_ground_truth": true,
    "has_eval_score": true,

    "early_stop_threshold": 0.8,
    "max_exceed_threshold_count": 3,
    "max_score_decline_count": 5,
    "use_early_stop_threshold": true,
    "use_early_stop_score_decline": true,

    "use_roll_back": true,

    "parallel": true,
    "parallel_max_num": 8,

    "initial_solution_path": "examples/hotpotqa/eval/accepted_solution/solution.json",
    "optimizer_config_path": "examples/hotpotqa/configs/optimizer_config.json",
    "optim_order": "order",
    "optimizers": [
        "sop"
    ],
    "additional_info": {
        "loss": "remember to tell the agents that the answers are typically between 1 to 5 words long.",
        "sop": "",
        "node": "",
        "prompt": ""
    },
    "wandb_config": {
        "project": "Agent2",
        "name": "hotpotqa",
        "mode": "online"
    }
}